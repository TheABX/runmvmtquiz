{
  "name": "lifephoria-quiz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:fresh": "pkill -f 'next dev' || true && pkill -f 'node.*next' || true && sleep 2 && rm -rf .next && npm run dev",
    "dev:safe": "bash scripts/dev-safe.sh",
    "dev:reset": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true; pkill -f 'next dev' 2>/dev/null || true; sleep 2; rm -rf .next node_modules/.cache; npm run dev",
    "build": "next build",
    "build:check": "bash scripts/validate-build.sh",
    "start": "next start",
    "lint": "next lint",
    "format:check": "bash scripts/check-formatting.sh",
    "pre-commit": "npm run format:check && npm run build:check"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/supabase-js": "^2.86.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^20.0.0",
    "styled-jsx": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
